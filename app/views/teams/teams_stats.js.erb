  $("#t_over").click(function(){
    $('#change_team_stats').html("<%= j render partial: 'team_overwiev', locals: {teams: @teams} %>");
  });
  $("#t_offe").click(function(){
    $('#change_team_stats').html("<%= j render partial: 'team_offensive', locals: {teams: @teams} %>");
  });
  $("#t_deff").click(function(){
    $('#change_team_stats').html("<%= j render partial: 'team_deffensive', locals: {teams: @teams} %>");
  });
  $("#t_book").click(function(){
    $('#change_team_stats').html("<%= j render partial: 'team_booking', locals: {teams: @teams} %>");
  });
  $("#p_over").click(function(){
    $('#change_player_stats').html("<%= j render partial: 'player_overwiev', locals: {players: @players} %>");
  });
  $("#p_offe").click(function(){
    $('#change_player_stats').html("<%= j render partial: 'player_offensive', locals: {players: @players} %>");
  });
  $("#p_deff").click(function(){
    $('#change_player_stats').html("<%= j render partial: 'player_deffensive', locals: {players: @players} %>");
  });
  $("#p_book").click(function(){
    $('#change_player_stats').html("<%= j render partial: 'player_booking', locals: {players: @players} %>");
  });
});
