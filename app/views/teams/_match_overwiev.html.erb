<h2>Latest games</h2>
<table class="table sts_table mtch_table">
  <tr>
    <th>Round</th>
    <th>Date</th>
    <th class="alg_left">VS</th>
    <th>Result</th>
  </tr>

  <% @matches.order(round: :desc).each do |match| %>
    <tr>
      <td><%= match.round %></td>
      <td><%= match.kick_off.slice(0,6) %></td>
      <td class="alg_left"><%= match.team_name %>-<%= match.opp_team_name %></td>
      <td><%= match.team_h_score %>:<%= match.team_a_score %></td>
    </tr>
  <% end %>
</table>
