<p id="notice"><%= notice %></p>

<h1>Player Matches</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @player_matches.each do |player_match| %>
      <tr>
        <td><%= link_to 'Show', player_match %></td>
        <td><%= link_to 'Edit', edit_player_match_path(player_match) %></td>
        <td><%= link_to 'Destroy', player_match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Player Match', new_player_match_path %>
